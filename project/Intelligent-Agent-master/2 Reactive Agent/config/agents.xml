	<agent name="reactive1">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive2">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".55"/>
	</agent>
	
	<agent name="reactive3">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
